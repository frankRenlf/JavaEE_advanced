<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.frank.demo.dao.UserDao">
    <select id="getAll" resultType="com.frank.demo.domain.UserInfo">
        select * from userinfo
    </select>
    <select id="getById" resultType="com.frank.demo.domain.UserInfo">
        select * from userinfo where id = ${id}
    </select>
</mapper>
